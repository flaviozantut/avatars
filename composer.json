{
    "name": "rodrigorigotti/avatars",
    "description": "avatars.io client",
    "keywords":["avatar","avatars.io", "gravatar", "laravel4", "laravel"],
    "homepage": "https://github.com/flaviozantut/avatars",
    "license": "MIT",
    "authors": [
        {
            "name": "flaviozantut",
            "email": "flaviozantut@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "guzzle/guzzle": "3.*"

    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "evert/phpdoc-md" : "0.0.*",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls":"~0.6"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/commands"
        ],
        "psr-0": {
            "Flaviozantut\\Avatars": "src/"
        }
    },
    "minimum-stability": "stable",
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
